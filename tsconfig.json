{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./src",
    "paths": {
      "*": ["types/*"] 
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["astro/client"],
    "noImplicitAny": false
  },
  "include": [
    "src",
    "src/types"
, "i18next.ts"  ]
}
